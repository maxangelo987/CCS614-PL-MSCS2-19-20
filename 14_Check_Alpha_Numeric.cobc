           ELSE IF STR1(5:1) IS NUMERIC
           DISPLAY STR1(5:1) " IS NUMERIC "
           ELSE 
           DISPLAY STR1(5:1) " IS ALPHANUMERIC " 
           END-IF.
            
           IF STR1(6:1) IS ALPHABETIC
           DISPLAY STR1(6:1) " IS ALPHABETIC  "
           ELSE IF STR1(6:1) IS NUMERIC
           DISPLAY STR1(6:1) " IS NUMERIC "
           ELSE 
           DISPLAY STR1(6:1) " IS ALPHANUMERIC " 
           END-IF.
        STOP RUN.
