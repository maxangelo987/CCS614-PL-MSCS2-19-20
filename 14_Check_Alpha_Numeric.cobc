           DISPLAY STR1(2:1) " IS NUMERIC "
           ELSE 
           DISPLAY STR1(2:1) " IS ALPHANUMERIC " 
           END-IF.
            
           IF STR1(3:1) IS ALPHABETIC
           DISPLAY STR1(3:1) " IS ALPHABETIC  "
           ELSE IF STR1(3:1) IS NUMERIC
           DISPLAY STR1(3:1) " IS NUMERIC "
           ELSE 
           DISPLAY STR1(3:1) " IS ALPHANUMERIC " 
           END-IF.
        
           IF STR1(4:1) IS ALPHABETIC
