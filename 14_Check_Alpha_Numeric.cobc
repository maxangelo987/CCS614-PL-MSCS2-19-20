           ELSE IF STR1(4:1) IS NUMERIC
           DISPLAY STR1(4:1) " IS NUMERIC "
           ELSE 
           DISPLAY STR1(4:1) " IS ALPHANUMERIC " 
           END-IF.
           
           IF STR1(5:1) IS ALPHABETIC
